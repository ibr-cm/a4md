<?xml version="1.0" encoding="UTF-8"?>
<services>
    <service type="artery.application.CaService">
        <listener port="2001"/>
        <filters operator="and">
            <type pattern="artery.inet.Person" match="inverse"/>
            <penetration rate="0.7"/>
        </filters>
    </service>
    <service type="artery.application.md.MisbehaviorCaService">
        <listener port="2001"/>
        <filters operator="and">
            <type pattern="artery.inet.Person" match="inverse"/>
            <service name="CaService" match="inverse"/>
        </filters>
    </service>
    <service type="artery.application.md.MisbehaviorDetectionService">
        <listener port="4712"/>
        <filters>
            <type pattern="artery.inet.Person" match="inverse"/>
        </filters>
    </service>
</services>

