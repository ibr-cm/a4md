<?xml version="1.0" encoding="UTF-8"?>
<services>
    <service type="artery.application.CaService">
        <listener port="2001"/>
        <filters operator="and">
            <type pattern="artery.envmod.Car"/>
        </filters>
    </service>
    <service type="artery.application.md.MisbehaviorCaService">
        <listener port="2001"/>
        <filters>
            <type pattern="artery.application.md.MisbehaviorCar"/>
        </filters>
    </service>
    <service type="artery.application.md.MisbehaviorDetectionService">
        <listener port="4712"/>
        <filters operator="or">
            <type pattern="artery.envmod.Car"/>
            <type pattern="artery.application.md.MisbehaviorCar"/>
        </filters>
    </service>
</services>

